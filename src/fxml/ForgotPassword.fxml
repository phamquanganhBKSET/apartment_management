<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="311.0" prefWidth="436.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ForgotPassword">
   <children>
      <Label fx:id="loginLabel" layoutX="85.0" layoutY="77.0" prefHeight="42.0" prefWidth="272.0" text="Forgot Password">
         <font>
            <Font name="Trebuchet MS" size="36.0" />
         </font></Label>
      <Button layoutX="82.0" layoutY="226.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="272.0" text="Request password reset">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <TextField id="username" fx:id="username" layoutX="82.0" layoutY="170.0" prefHeight="29.0" prefWidth="272.0" promptText="Email address">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label fx:id="close" layoutX="402.0" layoutY="7.0" onMouseClicked="#handleClose" prefHeight="30.0" prefWidth="26.0" text=" X">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="minimize" layoutX="364.0" layoutY="5.0" onMouseClicked="#handleMinimize" prefHeight="32.0" prefWidth="19.0" text=" -" textAlignment="CENTER">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label layoutX="155.0" layoutY="134.0" text="Enter your email address" />
      <ImageView fitHeight="63.0" fitWidth="83.0" layoutX="12.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../icon/forgot-password.png" />
         </image>
      </ImageView>
      <Hyperlink layoutX="181.0" layoutY="271.0" text="Back to Login" />
   </children>
</AnchorPane>
